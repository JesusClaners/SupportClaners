{
    "name": "Soporte Claners",
    "author": "Soporte Claners",
    "defaultLocale": "es",
    "location": {
      "support": {
        "background": "assets/iframe.html",
        "ticket_sidebar": {
          "autoLoad": false,
          "flexible": true
        },
        "top_bar": {
          "autoHide": true,
          "url": "assets/top_bar.html",
          "size": {
            "width": "450px",
            "height": "500px"
          }
        }
      }
    },
    "domainWhitelist": ["https://www.claners.com/"],
    "parameters": [
        {
          "name": "subdomain",
          "type": "text",
          "required": true
        },
        {
          "name": "useSSL",
          "type": "checkbox"
        }
      ],
    "version": "1.0",
    "frameworkVersion": "2.0"
  }